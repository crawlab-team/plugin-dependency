{
  "name": "dependency",
  "description": "A plugin for managing dependencies",
  "proto": "http",
  "cmd": "go run ./...",
  "endpoint": "localhost:9998",
  "event_key": {
    "include": "^model:",
    "exclude": "artifact"
  },
  "install_url": "https://github.com/crawlab-team/plugin-dependency",
  "deploy_mode": "all",
  "auto_start": true,
  "ui_components": [
    {
      "name": "dependency-settings",
      "title": "Dependencies",
      "src": "ui/src/DependencySettings.vue",
      "type": "view",
      "path": "dependencies/settings"
    },
    {
      "name": "dependency-python",
      "title": "Dependencies",
      "src": "ui/src/python/DependencyPython.vue",
      "type": "view",
      "path": "dependencies/python"
    },
    {
      "name": "dependency-node",
      "title": "Dependencies",
      "src": "ui/src/node/DependencyNode.vue",
      "type": "view",
      "path": "dependencies/node"
    }
  ],
  "ui_sidebar_navs": [
    {
      "path": "/dependencies",
      "title": "Dependencies",
      "icon": [
        "fa",
        "puzzle-piece"
      ],
      "children": [
        {
          "path": "/dependencies/settings",
          "title": "Settings",
          "icon": [
            "fa",
            "cogs"
          ]
        },
        {
          "path": "/dependencies/python",
          "title": "Python",
          "icon": [
            "fab",
            "python"
          ]
        },
        {
          "path": "/dependencies/node",
          "title": "Node",
          "icon": [
            "fab",
            "node-js"
          ]
        }
      ]
    }
  ],
  "ui_assets": []
}